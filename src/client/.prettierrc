{
    "trailingComma": "none",
    "semi": false,
    "tabWidth": 2,
    "useTabs": true,
    "printWidth": 240,
    "singleQuote": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "endOfLine": "auto",
    "arrowParens": "always",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "embeddedLanguageFormatting": "auto"
}